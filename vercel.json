{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "templates/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "index.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "app.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "templates/$1"
      }
    ]
  }